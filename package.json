{
  "name": "vite-plugin-graphql-server",
  "version": "1.2.0",
  "description": "Bootstrap a local GraphQL server in your Vite project",
  "author": "Mohammad Ataei",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/mammadataei/vite-plugin-graphql-server/"
  },
  "bugs": {
    "url": "https://github.com/mammadataei/vite-plugin-graphql-server/issues"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "vitest --ui --open=false",
    "coverage": "vitest run --coverage",
    "format": "prettier --write . && pnpm lint --fix",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,md,html,css}": "prettier --write"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^1.1.1",
    "@vitest/ui": "^1.1.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.10",
    "vitest": "^1.1.1"
  },
  "peerDependencies": {
    "graphql": "^16.0.0",
    "vite": "^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@graphql-tools/schema": "^10.0.2"
  },
  "packageManager": "pnpm@8.13.1"
}
